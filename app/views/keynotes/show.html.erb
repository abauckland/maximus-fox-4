<div class="row title_band">    <div class="page_title">        <h1>Download Keynote File</h1>    </div></div><!-- project information bar --><div class="row info_band">    <div class="info_right">        <div><h3>Status: <%= @project.project_status %></h3></div>        <div><h3>Rev: <%= current_revision_render(@project) %></div>    </div>    <div class="info_left">        <div class=""><h3><%= @project.code_and_title %></h3></div>    </div></div><div class="row">    <div class="col-1-3 column_1">        <div class="border">            <div class="row">            <form action="<%= keynote_export_keynote_path %>" method="get" >            	<%= hidden_field_tag "id", @project.id %>                        <fieldset>                        <div class="row">                            <div class="column_title">                                <h4>Keynote Format<span class="form_tooltip" id="keynote_format"></span></h4>                            </div>                        </div>                        <!-- form input[radio] -->                        <label class="label_80">File Format:</label>                        <span>                            <%= radio_button_tag "cad_product", "csv", checked = true %>                            <span>Spreadsheet (csv)</span>                        </span></br>                        <!-- form input[radio] -->                        <label class="label_80"></label>                        <span>                            <%= radio_button_tag "cad_product", "revit", checked = true %>                        	<span>Autodesk Revit (txt)</span>                        </span></br><!--                    <label class="label_120"></label>                    <span>                        <%= radio_button_tag "cad_product", "bentley"%>                    	<span>Bentley Microstation Keynote file (spc)</span>                    </span></br>-->                        <label class="label_80"></label>                        <span>                            <%= radio_button_tag "cad_product", "cadimage"%>                        	<span>Cadimage Archicad (csv)</span>                        </span></br><!--                    <label class="label_120"></label>                    <span>                        <%= radio_button_tag "cad_product", "cadimage_keynote"%>                    	<span>Cadimage Archicad Keynote file (keynote)</span>                    </span></br>-->                    </fieldset>            </div>			<!-- column footer start-->             <div class="row">                <div class="form_footer">                    <%= submit_tag "download"%>                </div>                </form>            </div>        </div>    </div>    <div class="col-1-3 column_2">        <div class="border blank_column"></div>    </div>    <div class="col-1-3 column_3">        <div class="border blank_column"></div>    </div></div>