<div class="row title_band">    <div class="page_title">        <h1>Download Clause List</h1>    </div>    <div class="title_help" id="keynote_show"></div></div><div class="project_info">	<div class="project_title">		<div class="project_code">       		Project: <%=h @project.code%>		</div>		<div class="project_code_title">       		<%=h @project.title %>		</div>	</div>		<div class="project_status">		Status: <%=h @project.project_status %>	</div>	<div class="project_rev">    	Rev: <%=h @revision.rev %>  	</div></div><div class="row">    <div class="col-1-3 column_1">        <div class="border">            <div class="row">            <form action="<%= keynote_export_keynote_path %>" method="get" >            	<%= hidden_field_tag "id", @project.id %>                        <fieldset>                    <!-- form input[radio] -->                    <div class="form_tooltip" id="keynote_format"></div>                    <label class="label_120">File Format:</label>                    <span>                        <%= radio_button_tag "cad_product", "revit", checked = true %>                    	<span>Autodesk Revit Keynote file (txt)</span>                    </span></br><!--                    <label class="label_120"></label>                    <span>                        <%= radio_button_tag "cad_product", "bentley"%>                    	<span>Bentley Microstation Keynote file (spc)</span>                    </span></br>-->                    <label class="label_120"></label>                    <span>                        <%= radio_button_tag "cad_product", "cadimage"%>                    	<span>Cadimage Archicad Keynote file (csv)</span>                    </span></br><!--                    <label class="label_120"></label>                    <span>                        <%= radio_button_tag "cad_product", "cadimage_keynote"%>                    	<span>Cadimage Archicad Keynote file (keynote)</span>                    </span></br>-->                    </fieldset>            </div>			<!-- column footer start-->             <div class="row">                <div class="form_footer">                    <%= f.submit "download"%>                </div>                <% end %>            </div>        </div>    </div>    <div class="col-1-3 column_2">        <div class="border blank_column"></div>    </div>    <div class="col-1-3 column_3">        <div class="border blank_column"></div>    </div></div>