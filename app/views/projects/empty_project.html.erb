<div class="page_title">Getting Started</div><div class="content">    <%if @not_used == true %>		<%= render :partial => "new_user_intro", :current_project => @project %>   	<%else%>        <div class="column_1">		<div class="column_content">       			<div class="column_title">Add content to <%= @project.code %></div>       			    		<div class="column_sub_content">				<div class="column_guide">					<p>This project does not have any content. Please click on the 'add sections' link below and copy into the project sections for the appropriate template.       			</div>      			    		</div>    	      		<div class="column_footer">        		<div>        			<div class="column_submit"><%= link_to "add sections", {:controller => "projects", :action => "manage_subsections"}%> </div>        		</div>         		</div>    		      	</div>   	</div>		<div class="column_2 column_blank"></div>     <div class="column_3 column_blank"></div>    <%end%> </div>      			