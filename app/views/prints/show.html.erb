<div class="project_info">	<div class="project_title">		<div class="project_code">       		Project: <%= @project.code%>		</div>		<div class="project_code_title">       		<%= @project.title %>		</div>	</div>		<div class="project_status">		Status: <%= @project.project_status %>	</div>	<div class="project_rev">    	Rev: <%= @revision.rev %>  	</div></div><div class="option_band_single">		<div class="project_select">						<%= print_revision_select_list(@project, @revisions, @revision)%>	</div></div><div class="content">    <div class="column_1">		<div class="column_content">      		<div class="column_title">Create PDF of Specification</div>             		<div class="column_sub_content">				<div class="column_guide">					<p>Where a project has been previously published. Previous revisions of the document can be downloaded by selecting the appropriate revision above</p>								<%= print_help(@print_status_show)%>				</div>								<div class="column_form">				<form action="<%= print_project_print_path %>" method="get" >						<%= hidden_field_tag "id", @project.id %>						<%= hidden_field_tag "revision", @revision.id %>						<table>						<tr><td>							<%= watermark_checkbox(@print_status_show) %>						</td></tr>						</table>	      	      			</div>												</div>						      		<div class="column_footer">        		<div>        			<div class="column_download"><%= submit_tag 'Create/Download Document'%></div>                      		</form>      		                     		</div>         		</div>      	</div>   	</div>	<div class="column_2">		<div class="column_content">      		<div class="column_title">Download Keynote File</div>             		<div class="column_sub_content">				<div class="column_guide">					<p>Select the keynote file format required.</p>				</div>								<div class="column_form">				<form action="<%= keynote_export_export_path %>" method="get" >						<%= hidden_field_tag "id", @project.id %>						<table>						<tr><td>							<%= radio_button_tag "cad_product", "revit", checked = true %>						</td>						<td>							<p>Autodesk Revit Keynote file (txt)</p>												</td></tr>						<tr><td>							<%= radio_button_tag "cad_product", "bentley" %>							</td>						<td>							<p>Bentley Microstation Keynote file (spc)</p>																		</td></tr>						<tr><td>							<%= radio_button_tag "cad_product", "cadimage" %>							</td>						<td>							<p>Cadimage Archicad Keynote file (spc)</p>												</td></tr>						</table>	      	      			</div>												</div>						      		<div class="column_footer">        		<div>        			<div class="column_download"><%= submit_tag 'Download Keynote File'%></div>                      		</form>      		                     		</div>         		</div>      	</div> 							</div>    <div class="column_3 column_blank"></div>  </div> 