<div class="row title_band">    <div class="page_title">        <h1><%=h @project.code_and_title %></h1>    </div>    <div class="title_help" id="printsetting_edit"></div></div><div class="row">    <div class="col-1-1">        <div class="border">            <%= render :partial => "form_error", :locals => {:item => @printsetting} %>            <%= form_for @printsetting do |f| %>                <%= f.hidden_field :company_id, :value => current_user.company_id %>                <div class="row">                <div class="sub-1-3 column_1">                    <fieldset>                        <div class="row">                            <div class="column_title">                                <h3>Document Settings<span class="form_tooltip"></span></h3>                            </div>                        </div>                        <!-- form input[select] -->                        <div class="form_tooltip" id="print_settings_font_style"></div>                        <label class="label_80">Font Style:</label>                        <span>                            <%= f.select :font_style, options_for_select(@font_styles, @printsetting.font_style) %>                        </span></br>                                                <!-- form input[select] -->                        <div class="form_tooltip" id="print_settings_font_size"></div>                        <label class="label_80">Font Size:</label>                        <span>                            <%= f.select :font_size, options_for_select(@font_sizes, @printsetting.font_size), {}, {disabled: true} %>                        </span></br>                                                <!-- form input[select] -->                        <div class="form_tooltip" id="print_settings_structure"></div>                        <label class="label_80">Revision Reporting:</label>                        <span>                            <%= f.select :structure, options_for_select(@structures, @printsetting.structure), {}, {disabled: true}  %>                        </span></br>                        <!-- form input[select] -->                        <div class="form_tooltip" id="print_settings_numbering"></div>                        <label class="label_80">Page Numbering:</label>                        <span>                            <%= f.select :page_number, options_for_select(@page_numbers, @printsetting.page_number), {}, {disabled: true}  %>                        </span></br>                        <!-- form input[select] -->                        <div class="form_tooltip" id="print_settings_section_cover"></div>                        <label class="label_80">Section Covers:</label>                        <span>                            <%= f.select :section_cover, options_for_select(@section_covers, @printsetting.section_cover), {}, {disabled: true}  %>                        </span></br>                    </fieldset>                </div>                <div class="sub-1-3 column_2">                    <fieldset>                        <div class="row">                            <div class="column_title">                                <h3>Document Cover<span class="form_tooltip"></span></h3>                            </div>                        </div>                        <!-- form input[select] -->                        <div class="form_tooltip" id="print_settings_client_logo"></div>                        <label class="label_80">Client Logo:</label>                        <span>                            <%= f.select :client_logo, options_for_select(@client_logos, @printsetting.client_logo) %>                        </span></br>                        <!-- form input[select] -->                        <div class="form_tooltip" id="print_settings_client_details"></div>                        <label class="label_80">Client Details:</label>                        <span>                            <%= f.select :client_detail, options_for_select(@client_details, @printsetting.client_detail) %>                        </span></br>                        <!-- form input[select] -->                        <div class="form_tooltip" id="print_settings_project_details"></div>                        <label class="label_80">Project Details:</label>                        <span>                            <%= f.select :project_detail, options_for_select(@project_details, @printsetting.project_detail) %>                        </span></br>                        <!-- form input[select] -->                        <div class="form_tooltip" id="print_settings_project_image"></div>                        <label class="label_80">Project Image:</label>                        <span>                            <%= f.select :project_image, options_for_select(@project_images, @printsetting.project_image) %>                        </span></br>                        <!-- form input[select] -->                        <div class="form_tooltip" id="print_settings_company_details"></div>                        <label class="label_80">Company Details:</label>                        <span>                            <%= f.select :company_detail, options_for_select(@company_details, @printsetting.company_detail) %>                        </span></br>                    </fieldset>                                                        </div>                <div class="sub-1-3 column_3">                    <fieldset>                        <div class="row">                            <div class="column_title">                                <h3>Headers/Footers<span class="form_tooltip"></span></h3>                            </div>                        </div>                        <!-- form input[select] -->                        <div class="form_tooltip" id="print_settings_project_title"></div>                        <label class="label_80">Project Title:</label>                        <span>                            <%= f.select :header_project, options_for_select(@header_projects, @printsetting.header_project) %>                        </span></br>                        <!-- form input[select] -->                        <div class="form_tooltip" id="print_settings_client_name"></div>                        <label class="label_80">Client Name:</label>                        <span>                            <%= f.select :header_client, options_for_select(@header_clients, @printsetting.header_client) %>                        </span></br>                        <!-- form input[select] -->                        <div class="form_tooltip" id="print_settings_document_name"></div>                        <label class="label_80">Document Name:</label>                        <span>                            <%= f.select :header_document, options_for_select(@header_documents, @printsetting.header_document) %>                        </span></br>                        <!-- form input[select] -->                        <div class="form_tooltip" id="print_settings_company_logo"></div>                        <label class="label_80">Company Logo:</label>                        <span>                            <%= f.select :header_logo, options_for_select(@header_logos, @printsetting.header_logo) %>                        </span></br>                        <!-- form input[select] -->                        <div class="form_tooltip" id="print_settings_project_details"></div>                        <label class="label_80">Project Details:</label>                        <span>                            <%= f.select :footer_detail, options_for_select(@footer_details, @printsetting.footer_detail)%>                        </span></br>                        <!-- form input[select] -->                        <div class="form_tooltip" id="print_settings_company_name"></div>                        <label class="label_80">Company Name:</label>                        <span>                            <%= f.select :footer_author, options_for_select(@footer_authors, @printsetting.footer_author)%>                        </span></br>                        <!-- form input[select] -->                        <div class="form_tooltip" id="print_settings_date_created"></div>                        <label class="label_80">Date Created:</label>                        <span>                            <%= f.select :footer_date, options_for_select(@footer_dates, @printsetting.footer_date) %>                        </span></br>                    </fieldset>                </div>            </div>                        <div class="row">                <div class="form_footer">                    <%= f.submit "save" %>                    <%= link_to "Cancel", edit_project_path(@project) %>                </div>                <% end %>            </div>                    </div>    </div></div>										